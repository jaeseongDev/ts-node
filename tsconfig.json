{
	"compilerOptions": {
		"target": "es6", // ts 파일을 js로 컴파일 할 때, es6 문법으로 컴파일한다. 
		"module": "commonjs", // (기본값)
		"outDir": "./dist", // 컴파일된 파일들을 담아줄 디렉토리를 설정
		"rootDir": "./src", // 컴파일할 디렉토리를 설정
		"moduleResolution": "node", 
		"esModuleInterop": true, // (기본값)
		"forceConsistentCasingInFileNames": true, // (기본값)
		"strict": true, // (기본값)
		"skipLibCheck": true // (기본값)
	}
}